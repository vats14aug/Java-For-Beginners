
// A java program consisting all methoda static , non static //
//including Switch case//
import java.util.Scanner;
class Switch
{
static int d;
static int e;
public int add(int a, int b)
{
return (a+b);
}
public static int sub(int a , int b)
{
return(a-b);
}
public static void mul(int a , int b)
{
 d=(a*b);
}
public void div(int a , int b)
{
 e=(a/b);
}
public static void main(String []args)
{
Scanner sc=new Scanner(System.in);		
System.out.println("Enter two numbers");
int a=sc.nextInt();
int b=sc.nextInt();
Switch S=new Switch();
mul(a,b);
S.div(a,b);
System.out.println("Enter 1: for add , \n2: for sub , 3: for mul and 4: for division");
int ch=sc.nextInt();
switch(ch)
{
case 1: System.out.println("Add is"+S.add(a,b));
	break;
case 2: System.out.println("Sub is"+sub(a,b));
break;
case 3: System.out.println("Mul is"+d);
break;
case 4: System.out.println("Div is"+e);
}
}
}
